{
  "version": 2,
  "name": "realtime-anomaly-detection",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "silent": true
  },
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}